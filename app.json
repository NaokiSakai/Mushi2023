{
  "expo": {
    "name": "ムシマップワールド",
    "slug": "mushi_map_world2023",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ムシ.webp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ムシ.webp",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA_ROLL"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
